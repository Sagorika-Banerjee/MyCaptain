#include <stdio.h>

int main() {
  int marks;
  printf("Enter marks: ");
  scanf("%d", &marks);
  if ( marks >= 85 && marks <= 100 )
    printf("Your marks is- %d, thus your grade is- Grade A\n", marks);
  else if ( marks >= 70 && marks <= 84 )
    printf("Your marks is- %d, thus your grade is- Grade B\n", marks);
  else if ( marks >= 55 && marks <= 69 )
    printf("Your marks is- %d, thus your grade is- Grade C\n", marks);
  else if ( marks >= 40 && marks <= 54 )
    printf("Your marks is- %d, thus your grade is- Grade D\n", marks);
  else
    printf("Your marks is- %d, thus your grade is- Grade F\n", marks);
  return 0;
}
